<% if current_user %>
    <%= link_to "Log Out", logout_path %>
    <section class='existing-users'>
        <p>Existing Users: </p>
        <% @users.each do |user| %>
            <li><%= user.email %></li>
        <% end %>
    </section>
<% else %>
    <%= button_to "Create New User", register_path, method: :get %>
    <%= link_to "Log In", login_path %>
<% end %>

